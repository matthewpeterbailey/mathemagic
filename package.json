{
  "name": "mathemagic",
  "version": "1.0.9",
  "description": "A mathematics and statistic library for Node.js (javascript and typescript)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewpeterbailey/mathemagic.git"
  },
  "keywords": [
    "Mathematics"
  ],
  "author": "Matthew Bailey <mpb1992@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matthewpeterbailey/mathemagic/issues"
  },
  "homepage": "https://github.com/matthewpeterbailey/mathemagic#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  },
  "files": [
    "lib/**/*"
  ]
}
